{
	"name": "zenscript",
	"displayName": "zenscript",
	"description": "Syntax highliting for ZS",
	"version": "0.0.3",
	"publisher": "Mrthomas20121",
	"repository": {
		"type": "git",
		"url": "https://github.com/CraftTweaker/ZsLint"
	},
	"icon": "assets/zslint_logo.png",
	"galleryBanner": {
		"color": "#D3D3D3"
	},
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Linters"
	],
	"devDependencies": {
		"@types/node": "^7.0.43",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.6"
	},
	"activationEvents": [
		"onLanguage:zenscript"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"contributes": {
		"languages": [
			{
				"id": "zenscript",
				"aliases": [
					"ZenScript",
					"zenscript"
				],
				"extensions": [
					".zs"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "zenscript",
				"scopeName": "source.zenscript",
				"path": "./syntaxes/zenscript.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "ZenScript",
				"path": "./snippets/zenscript.json"
			},
			{
				"language": "ZenScript",
				"path": "./snippets/neisupport.json"
			}
		]
	},
	"dependencies": {
		"@types/ws": "^5.1.1",
		"npm": "^6.0.1",
		"ws": "^5.1.1"
	},
	"__metadata": {
		"id": "964e44a7-4fb0-44e0-8aaf-2da5430213a2",
		"publisherId": "45e1de01-1e43-424a-b308-d3d036dee26b",
		"publisherDisplayName": "BloodWorkXGaming"
	}
}