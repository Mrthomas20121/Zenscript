{
	"name": "zenscript",
	"displayName": "zenscript",
	"description": "Syntax highlighting for Zenscript",
	"version": "1.0.4",
	"publisher": "Mrthomas20121",
	"author": {
		"name": "Mrthomas20121",
		"email": "Mrthomas20121@live.com"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Mrthomas20121/Zenscript"
	},
	"icon": "assets/Zenscript_logo.png",
	"galleryBanner": {
		"color": "#D3D3D3"
	},
	"categories": [
		"Programming Languages"
	],
	"devDependencies": {
		"@types/node": "^7.10.9",
		"@types/vscode": "^1.39.0",
		"@types/ws": "^5.1.2",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1"
	},
	"activationEvents": [
		"onLanguage:zenscript"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "npm run compile"
	},
	"contributes": {
		"languages": [
			{
				"id": "zenscript",
				"aliases": [
					"ZenScript",
					"zenscript"
				],
				"extensions": [
					".zs"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "zenscript",
				"scopeName": "source.zenscript",
				"path": "./syntaxes/zenscript.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "zenscript",
				"path": "./snippets/zenscript.json"
			}
		]
	},
	"dependencies": {
		"npm": "^10.8.1",
		"vscode-test": "^1.2.2",
		"ws": "^5.2.4"
	},
	"typings": "./src/api/crafttweaker.ts",
	"license": "MIT"
}
